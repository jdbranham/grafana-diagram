{
  "name": "grafana-diagram",
  "version": "1.9.0",
  "description": "Display diagrams and charts in Grafana with colored metric indicators",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit"
  },
  "homepage": "https://github.com/jdbranham/grafana-diagram#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdbranham/grafana-diagram.git"
  },
  "author": "Jeremy Branham <jeremy@savantly.net> (https://github.com/jdbranham)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jdbranham/grafana-diagram/issues",
    "email": "bugs@savantly.net"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@emotion/css": "^11.1.3",
    "@grafana/e2e": "9.5.2",
    "@grafana/e2e-selectors": "9.5.2",
    "@grafana/eslint-config": "^5.1.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "^1.3.51",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.4",
    "@types/d3": "^6.7.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/mermaid": "^8.2.1",
    "@types/node": "^18.15.11",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "emotion": "10.0.27",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.62.1",
    "sass-loader": "13.2.2",
    "style-loader": "3.3.2",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "peerDependencies": {
    "lodash": "^4.17.11"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@grafana/data": "9.2.0",
    "@grafana/runtime": "9.5.2",
    "@grafana/ui": "9.2.0",
    "mermaid": "9.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "2.5.0"
  },
  "engines": {
    "node": ">=14"
  },
  "packageManager": "yarn@1.22.19"
}
